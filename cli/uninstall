#!/bin/bash

filename=$PWD
homepath="${filename%FENGSim*}"
homepath+=FENGSim
echo $homepath
cd $homepath
cd cli

rm -rf $PWD/../toolkit/Geometry/vtk/build
rm -rf $PWD/../toolkit/Geometry/install/vtk_install
rm -rf $PWD/../toolkit/Geometry/oce/build
rm -rf $PWD/../toolkit/Geometry/install/oce_install
rm -rf $PWD/../toolkit/Geometry/gmsh/build
rm -rf $PWD/../toolkit/Geometry/install/gmsh_install
rm -rf $PWD/../toolkit/Geometry/cgal/build
rm -rf $PWD/../toolkit/Geometry/install/cgal_install
rm -rf $PWD/../toolkit/Geometry/pcl/build
rm -rf $PWD/../toolkit/Geometry/install/pcl_install

rm -rf $PWD/../toolkit/Tools/qt/5.12.12
rm -rf $PWD/../toolkit/Tools/qt/icu
rm -rf $PWD/../toolkit/Tools/qt/qtcreator/bin
rm -rf $PWD/../toolkit/Tools/qt/qtcreator/lib
rm -rf $PWD/../toolkit/Tools/qt/qtcreator/libexec
rm -rf $PWD/../toolkit/Tools/qt/qtcreator/share

rm -rf $PWD/../starter/build*
rm -rf $PWD/../starter/FENGSim/*.pro.user
rm -rf $PWD/../starter/ALE/build
rm -rf $PWD/../starter/CGAL/build
rm -rf $PWD/../starter/CGAL/data/output
rm -rf $PWD/../starter/GDT/build
